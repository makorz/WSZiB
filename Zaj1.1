public class first {
    // We'll say that a number is "teen" if it is in the range 13..19 inclusive. Given 2 int values, return true if
    // one or the other is "teen", but not both
    // HELLO

    public static void main(String[] args) {

        teens(13,19);
        teens(12,20);
        teens(15,25);

    }

    public static boolean teens(int x, int y){

        if ((x >= 13 && x <= 19) && (y >= 13 && y <= 19)){
            System.out.println("False!");
            return false;
        }else if ((x < 13 || x > 19) && (y < 13 || y > 19)){
            System.out.println("False!");
            return false;
        }
        System.out.println("True!");
        return true;

    }
}
