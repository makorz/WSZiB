public class third {
    // Given an array of ints, return true if the sequence of numbers 1, 2, 3 appears in the array somewhere

    public static void main(String[] args) {

        int[] tab_1 = {1,5,3,5,8,7,1,2,3,5};
        int[] tab_2 = {0,-2,5,13,1,12,54,1,2};
        System.out.println("First array checking:");
        sequenceFinder(tab_1);
        System.out.println("Second array checking:");
        sequenceFinder(tab_2);

    }

    public static boolean sequenceFinder(int array[]){

        for(int i = 0; i < array.length; i++) {
            if ((array[i] == 1) && ((i+1) < array.length) && (array[i+1] == 2) &&
                    ((i+2) < array.length) && (array[i+2] == 3)) {
                System.out.println("Got it!");
                return true;
            }
        }
        System.out.println("Sequence not found!");
        return false;
    }
}
